{
  "name": "app.zoomin.mono",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm fe:start:nolint\" \"pnpm --filter ./packages/backend dev\"",
    "start": "concurrently \"pnpm fe:start:nolint\" \"pnpm --filter ./packages/backend start\"",

    "fe:start": "pnpm --filter ./packages/frontend start",
    "fe:start:nolint": "cross-env DISABLE_ESLINT_PLUGIN=true pnpm --filter ./packages/frontend start",
    "fe:build": "pnpm --filter ./packages/frontend build",
    "fe:build:nolint": "cross-env DISABLE_ESLINT_PLUGIN=true pnpm --filter ./packages/frontend build",
    "fe:test": "pnpm --filter ./packages/frontend test",
    "fe:updatebuild": "pnpm --filter ./packages/frontend updatebuild",

    "be:dev": "pnpm --filter ./packages/backend dev",
    "be:start": "pnpm --filter ./packages/backend start",
    "be:test": "pnpm --filter ./packages/backend test",
    "be:websocket-test": "pnpm --filter ./packages/backend websocket-test",
    "be:deploy:prod": "pnpm --filter ./packages/backend deploy-prod",
    "be:deploy:staging": "pnpm --filter ./packages/backend deploy-staging"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3"
  }
}
